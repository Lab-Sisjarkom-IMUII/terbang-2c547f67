# Copy this to `.env` and fill values locally (DO NOT commit your real keys)
VITE_GOOGLE_CLIENT_ID=your-google-client-id-here
OPENAI_API_KEY=your-openai-api-key-here

# Supabase configuration
# For server (use service role key) - create a .env in project root for server
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_SERVICE_KEY=service_role_or_server_key_here

# For frontend (Vite) - create .env.local with VITE_ prefixed keys
VITE_SUPABASE_URL=https://your-project-ref.supabase.co
VITE_SUPABASE_ANON_KEY=public_anon_key_here
PORT=3001
